<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="Assign Technician" contentWidth="300px">
        <content>
            <VBox class="sapUiSmallMargin">
                <Label text="Select Technician" />
                <Select id="techSelect" items="{maintenance>/technicians}" width="100%">
                    <core:Item key="{maintenance>key}" text="{maintenance>text}" />
                </Select>
            </VBox>
        </content>
        <beginButton>
            <Button text="Assign" type="Emphasized" press=".onConfirmAssign" />
        </beginButton>
        <endButton>
            <Button text="Cancel" press=".onCancelAssign" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
